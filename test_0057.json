      },
      "expected": [[1, 2], [3, 10], [12, 16]]
    },
    {
      "args": {
        "intervals": [[1, 3], [6, 9]],
        "newInterval": [0, 10]
      },
      "expected": [[0, 10]]
    },
    {
      "args": {
        "intervals": [],
        "newInterval": [5, 7]
      },
      "expected": [[5, 7]]
    },
    {
      "args": {
        "intervals": [[1, 5]],
        "newInterval": [6, 8]
      },
      "expected": [[1, 5], [6, 8]]
    },
    {
      "args": {
        "intervals": [[1, 3], [7, 9]],
        "newInterval": [5, 6]
      },
      "expected": [[1, 3], [5, 6], [7, 9]]
